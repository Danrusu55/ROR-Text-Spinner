<div class="container">
  <div class="col-sm-10 col-sm-offset-1 col-xs-12 blog-content">
    <h1> Generated Articles: </h1>
    <div class="col-xs-12">
      <% @articleList.each_with_index do |article,index| %>
        <h2>Article #: <%= index + 1 %> </h2>
        <p><%= article['title'] %><br><br></p>
        <p><%= simple_format(article['body']) %><br><br></p>
        <p><%= article['keywords'] %><br><br></p>
      <% end %>
      <br>
    </div>
  </div>
</div>
